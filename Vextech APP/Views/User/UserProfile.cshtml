@model Vextech_APP.ViewModels.UserModels.UserViewModel

@{
    ViewData["Title"] = "User Profile";
}
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Firstname)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Firstname)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Lastname)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Lastname)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.PhoneNumbers)
        </dt>
        <dd class = "col-sm-10">
            @foreach (var item in Model.PhoneNumbers)
            {
                 <p>@item.PhoneNumber - (@item.mobileCategory.Name)</p>
            }
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new { /* id = Model.PrimaryKey */ }) | @Html.ActionLink("Delete", "UserDelete") |
    <a asp-controller="App" asp-action="Index">Back to Homepage</a>
</div>
